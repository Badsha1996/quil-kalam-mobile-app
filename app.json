{
  "expo": {
    "name": "quilkalam",
    "slug": "quilkalam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/solid-bg.jpg",
    "scheme": "quilkalam",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/transparent-bg.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/transparent-bg.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/images/transparent-bg.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "mdpi": "./assets/images/transparent-bg.png",
        "hdpi": "./assets/images/transparent-bg.png",
        "xhdpi": "./assets/images/transparent-bg.png",
        "xxhdpi": "./assets/images/transparent-bg.png",
        "xxxhdpi": "./assets/images/transparent-bg.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/images/transparent-bg.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/transparent-bg.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
